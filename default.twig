<head>
	<meta charset="utf-8" />
	<title>{{ page.title }}</title>
	<meta name="keywords" content="{{ page.keywords }}" />
	<meta name="description" content="{{ page.description }}" />
	<meta http-equiv="content-language" content="{{ page.seoLang }}" />

	{% if site.favicon %}
		<link rel="icon" href="{{ site.favicon['favicon'] }}" type="image/x-icon" />
		<link rel="SHORTCUT ICON" href="{{ site.favicon['thumbnail'] }}?v={{ "now"|date("U") }}" type="image/x-icon" />
	{% endif %}
	<meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
	<meta name="viewport" content="width=device-width,initial-scale=1">

	{% include basekit.headScript %}
</head>
<body class="{{ page.backgroundClass }}">

	<!-- Container -->
  <header class="bk-header">  
   <div class="bk-pt-default">
<div class="header-bg">
         <div class="wrapper companyheaderwrap clearfix">
            {{widget('logo', 'logo', {"defaultlogo": asset('images/logo.png')})|raw}}
        </div>

    <center><nav class="bk-nav">
        <div class="wrapper clearfix">
            {{widget('extendednavigation', 'navigationheader', {})|raw}}
        </div>
    </nav></center>
    
    </div>
    <section>
    {{widget('responsiveslideshow', 'thisunqiuewidgetname', {'duration':'3', 'transSpeed': '1000', 'transType': 'fade', 'showButtons': true, 'showBullets': false, 'allowPause': false, 'allowLoop': true, 
    'images':[
    {'url':'/images/slideshow/slideshow-img1.jpg', 'title':'test title', 'description': 'test desc'},
    {'url':'/images/slideshow/slideshow-img2.jpg', 'title':'test title', 'description': 'test desc'},
    {'url':'/images/slideshow/slideshow-img3.jpg', 'title':'test title', 'description': 'test desc'}
    ]})|raw}}
    </section>
    <!--<section>
    
{{widget('responsiveslideshow', 'thisunqiuewidgetname', {'duration':'3', 'transSpeed': '1000', 'transType': 'fade', 'showButtons': true, 'showBullets': false, 'allowPause': false, 'allowLoop': true, 
'images':[
